#include <iostream>
using namespace std;
int main() {
	int i;
	// comparando while com for
	i = 1;
	while (i <= 5){
		cout << i << endl;
		i = i +1 ;
	}
	// conta de 1 ate 5 com o for 
	for(i=1; i<=5; i = i+1){
		cout << i << endl;
	}
	
	cout << "\ndigite algo para terminar... " ; 
	string seguratela; cin >> seguratela;
	return 0;
}
